/* /////////////////////////////////////////////////////////////////////////////////////////////////
Style Label */
#labelRealStats {
    font: 12pt &quot;Segoe UI&quot;;
    color: white; /* warna teks */
    background-color: #134686; /* warna latar belakang */
    border-radius: 8px; /* ujung melengkung */
    padding: 6px 10px; /* jarak teks dari tepi */
}



QWidget{
	color: rgb(221, 221, 221);
	font: 10pt &quot;Segoe UI&quot;;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
	color: #ffffff;
	background-color: rgba(0, 63, 125, 180);
	border: 1px solid #005C99;
	background-image: none;
	background-position: left center;
	background-repeat: no-repeat;
	border: none;
	border-left: 2px solid #0091E5;
	text-align: left;
	padding-left: 8px;
	margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {	
	background-color: #002B5B;
	border: 1px solid #003F7D;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {	
	background-color: #003F7D;
}
#topLogo {
	background-color: #003F7D;
	background-image: url(:/images/images/images/logoecolab.png);
	background-position: centered;
	background-repeat: no-repeat;
}
#titleLeftApp { 
    font: 63 12pt &quot;Segoe UI Semibold&quot;; 
    color: #FFFFFF; 
}
#titleLeftDescription { 
    font: 8pt &quot;Segoe UI&quot;; 
    color: #77BEF0;
}



/* MENUS */
#topMenu .QPushButton {	
	background-position: left center;
	background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
	font-weight: 700;
}
#topMenu .QPushButton:hover {
	background-color: #005C99;
}
#topMenu .QPushButton:pressed {	
	background-color: #0091E5;
	color: rgb(255, 255, 255);
}
#bottomMenu .QPushButton {	
	background-position: left center;
	background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: #005C99;
}
#bottomMenu .QPushButton:pressed {	
	background-color: #0091E5;
	color: rgb(255, 255, 255);
}
#leftMenuFrame{
	border-top: 3px solid #0074CC;
}

/* Toggle Button */
#toggleButton {
	background-position: left center;
	background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: #002B5B;
	text-align: left;
	padding-left: 44px;
	color: rgb(210, 230, 255);
	font-weight: 700;
}
#toggleButton:hover {
	background-color: #003F7D;
}
#toggleButton:pressed {
	background-color: #0074CC;
}

/* Title Menu */
#titleRightInfo { padding-left: 10px; }
#titleRightInfo { 
    font: 12pt &quot;Segoe UI&quot;; 
    color: #FFFFFF;
	font-weight: 700;
}

#clockInfo { 
    font: 10pt &quot;Segoe UI&quot;; 
    color: #FFFFFF;

}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Extra Tab */
#extraLeftBox {	
	background-color: #003F7D;
}
#extraTopBg{	
	background-color: #0091E5;
}

/* Icon */
#extraIcon {
	background-position: center;
	background-repeat: no-repeat;
	background-image: url(:/icons/images/icons/icon_settings.png);
}

/* Label */
#extraLabel { color: rgb(255, 255, 255); }

/* Btn Close */
#extraCloseColumnBtn { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#extraCloseColumnBtn:hover { background-color: #005C99; border-style: solid; border-radius: 4px; }
#extraCloseColumnBtn:pressed { background-color: #0074CC; border-style: solid; border-radius: 4px; }

/* Extra Content */
#extraContent{
	border-top: 3px solid #003F7D;
}

/* Extra Top Menus */
#extraTopMenu .QPushButton {
	background-position: left center;
	background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#extraTopMenu .QPushButton:hover {
	background-color: #005C99;
}
#extraTopMenu .QPushButton:pressed {	
	background-color: #0091E5;
	color: rgb(255, 255, 255);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Content App */
#contentTopBg{	
	background-color: #002B5B;
}
#contentBottom{
	border-top: 3px solid #005C99;
	/* background-color: #E6F4FF; âœ… biru muda mendekati putih */
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #E1F2FB,   /* atas */
        stop:1 #F1F9F9    /* bawah */
    );
}





/* Top Buttons */
#rightButtons .QPushButton { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#rightButtons .QPushButton:hover { background-color: #003F7D; border-style: solid; border-radius: 4px; }
#rightButtons .QPushButton:pressed { background-color: #002B5B; border-style: solid; border-radius: 4px; }

/* Theme Settings */
#extraRightBox { background-color: #003F7D; }
#themeSettingsTopDetail { background-color: #0091E5; }

/* Bottom Bar */
#bottomBar { background-color: #003F7D; }
#bottomBar QLabel { font-size: 11px; color: rgb(210, 230, 255); padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }

/* CONTENT SETTINGS */
/* MENUS */
